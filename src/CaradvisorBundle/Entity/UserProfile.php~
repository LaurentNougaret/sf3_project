<?php

namespace CaradvisorBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * UserProfile
 *
 * @ORM\Table(name="user_profile")
 * @ORM\Entity(repositoryClass="CaradvisorBundle\Repository\UserProfileRepository")
 */
class UserProfile
{
    /**
     * @var int
     *
     * @ORM\Column(name="id", type="integer")
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="AUTO")
     */
    private $id;

    /**
     * @var string
     *
     * @ORM\Column(name="something", type="string", length=255)
     */
    private $something;

    /**
     * @ORM\OneToOne(targetEntity="CaradvisorBundle\Entity\User")
     * @ORM\JoinColumn(name="answer_id", referencedColumnName="id")
     */
    private $user;


    /**
     * Get id
     *
     * @return int
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set something
     *
     * @param string $something
     *
     * @return UserProfile
     */
    public function setSomething($something)
    {
        $this->something = $something;

        return $this;
    }

    /**
     * Get something
     *
     * @return string
     */
    public function getSomething()
    {
        return $this->something;
    }
}

