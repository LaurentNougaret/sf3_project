{% extends "@Caradvisor/Admin/index.html.twig" %}
{% block body %}

    <h1>Register Admin</h1>
{{ form_start(form) }}
    {{ form_row(form.username) }}
    {{ form_row(form.email) }}
    {{ form_row(form.plainPassword.first) }}
    {{ form_row(form.plainPassword.second) }}

<button type="submit">Register!</button>
{{ form_end(form) }}

{% endblock %}